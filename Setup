local mainFolder = game.Workspace.QveryStartMenu_V2
local objectsFolder = mainFolder.Objects
local lighting = game.Lighting

warn("Loading Menu")

mainFolder.MainMenuConfig.Parent = objectsFolder.MainMenuGui
objectsFolder.MainMenuEvents.Parent = game.ReplicatedStorage
objectsFolder.MainMenuSpawnItems.Parent = game.Workspace
objectsFolder.MainMenuServerScripts.Parent = game.ServerScriptService
objectsFolder.MainMenuCharacterScripts.Parent = game.StarterPlayer.StarterCharacterScripts
objectsFolder.MainMenuGui.Parent = game.StarterGui
if not lighting:FindFirstChild("Atmosphere") then
	objectsFolder.MainMenuLighting.Atmosphere.Parent = lighting
elseif not lighting:FindFirstChild("Bloom") then
	objectsFolder.MainMenuLighting.Bloom.Parent = lighting
elseif not lighting:FindFirstChild("Blur") then
	objectsFolder.MainMenuLighting.Blur.Parent = lighting
elseif not lighting:FindFirstChild("ColorCorrection") then
	objectsFolder.MainMenuLighting.ColorCorrection.Parent = lighting
elseif not lighting:FindFirstChild("DepthOfField") then
	objectsFolder.MainMenuLighting.DepthOfField.Parent = lighting
elseif not lighting:FindFirstChild("SunRays") then
	objectsFolder.MainMenuLighting.SunRays.Parent = lighting
end
objectsFolder.MainMenuLighting:Destroy()

warn("Menu Loaded!")
